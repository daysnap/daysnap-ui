{
  "name": "@daysnap/widgets",
  "version": "0.0.1",
  "description": "daysnap react ui or widget",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "entry": "node bin/entry",
    "create": "node bin/create",
    "remove": "node bin/remove",
    "dev": "dumi dev",
    "storybook:dev": "start-storybook -p 6006 -s ./.storybook/public",
    "storybook:build": "build-storybook -s ./.storybook/public",
    "storybook:deploy": "gh-pages -d storybook-static",
    "tsc": "tsc",
    "pub": "node bin/pub",
    "clean": "rimraf lib esm dist",
    "build:site": "rimraf doc-site && dumi build",
    "preview:site": "cross-env SITE_BUILD_ENV=PREVIEW npm run build:site && serve doc-site",
    "deploy:site": "npm run build:site && gh-pages -d doc-site",
    "build": "npm run clean && npm run build:types && gulp",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysnap/daysnap-widgets.git"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.scss"
  ],
  "keywords": [
    "daysnap",
    "react",
    "widget",
    "ui"
  ],
  "author": "Ajuan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daysnap/daysnap-widgets/issues"
  },
  "homepage": "https://github.com/daysnap/daysnap-widgets#readme",
  "devDependencies": {
    "dumi": "^1.1.38",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
