{
  "name": "@daysnap/widgets",
  "version": "0.0.1",
  "description": "daysnap react ui or widget",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "entry": "plop --plopfile ./bin/entry.js --force",
    "create": "plop --plopfile ./bin/create.js",
    "remove": "plop --plopfile ./bin/remove.js",
    "deploy": "npm run build && node bin/deploy.js",
    "clean": "rimraf lib es dist",
    "dev": "dumi dev",
    "build:site": "rimraf doc-site && dumi build",
    "preview:site": "cross-env SITE_BUILD_ENV=PREVIEW npm run build:site && serve doc-site",
    "deploy:site": "gh-pages -d doc-site",
    "build": "npm run entry && npm run build:types && gulp",
    "build:types": "npm run clean && tsc -p tsconfig.build.json && cpr lib es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysnap/daysnap-widgets.git"
  },
  "keywords": [
    "daysnap",
    "react",
    "widget",
    "ui"
  ],
  "files": [
    "lib",
    "es"
  ],
  "author": "Ajuan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daysnap/daysnap-widgets/issues"
  },
  "homepage": "https://github.com/daysnap/daysnap-widgets#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.0",
    "@types/autosize": "^4.0.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-is": "^17.0.3",
    "@umijs/plugin-sass": "^1.1.1",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-import": "^1.13.3",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.38",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-sass": "^5.1.0",
    "node-dir": "^0.1.17",
    "ora": "^6.0.1",
    "plop": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "serve": "^13.0.2",
    "through2": "^4.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "async-validator": "^4.0.7",
    "autosize": "^5.0.1",
    "classnames": "^2.3.1",
    "dom-align": "^1.12.2",
    "react-is": "^17.0.2"
  },
  "resolutions": {
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
